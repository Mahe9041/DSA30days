#include <iostream>

using namespace std;

int prtn(int i,int n)
{
    if(i>n)
    return 0;
    cout<<i;
    prtn(++i,n);
    
}

int main()
{
    int n;
    cin>>n;
    prtn(1,n);
    return 0;
}

