#include <iostream>

using namespace std;
int name(int i,int n)
{
    if(i>n)
    {
        return 0;
    }
    cout<<"S Mahesh";
    name(i+1,n);
}
int main()
{
    int n;
    cin>>n;
    name(1,n);
    return 0;
}
