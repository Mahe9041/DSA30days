#include <iostream>

using namespace std;

int prtn(int i,int n)
{
    if(i<1)
    return 0;
    
    prtn(i-1,n);
    cout<<i;
}

int main()
{
    int n;
    cin>>n;
    prtn(n,n);
    return 0;
}

